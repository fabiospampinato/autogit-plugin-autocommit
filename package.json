{
  "name": "autogit-plugin-autocommit",
  "description": "A plugin for automatically making a commit when specific files change.",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck",
    "compile:watch": "tsc --skipLibCheck --watch",
    "lint": "tslint \"src/**/*.ts*\" --force",
    "prepublish": "npm run clean && npm run lint && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/autogit-plugin-autocommit/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/autogit-plugin-autocommit.git"
  },
  "keywords": [
    "autogit",
    "plugin",
    "autocommit"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "simple-git": "^1.104.0"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "rimraf": "^2.6.0",
    "tslint": "^4.4.2",
    "typescript": "^3.0.3"
  }
}
