{
  "name": "autogit-plugin-autocommit",
  "description": "A plugin for automatically making a commit when specific files change.",
  "version": "1.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/autogit-plugin-autocommit/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/autogit-plugin-autocommit.git"
  },
  "keywords": [
    "autogit",
    "plugin",
    "autocommit"
  ],
  "dependencies": {
    "@types/node": "^10.12.8",
    "chalk": "^2.4.1",
    "simple-git": "^1.104.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.0",
    "typescript": "^3.0.3",
    "typescript-transform-export-interop": "^1.0.1"
  }
}
